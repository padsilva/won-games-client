{
  "name": "nextjs-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --ext .jsx,.js",
    "test": "jest",
    "test:ci": "jest --runInBand",
    "generate": "yarn plop --plopfile ./generators/plopfile.js",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test dev 3000 cy:open",
    "test:e2e:ci": "start-server-and-test start 3000 cy:run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": [
      "eslint --ext .jsx,.js src --fix",
      "jest --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "@stripe/react-stripe-js": "^1.4.1",
    "@stripe/stripe-js": "^1.15.1",
    "@styled-icons/boxicons-solid": "^10.34.0",
    "@styled-icons/fa-brands": "^10.34.0",
    "@styled-icons/material-outlined": "^10.34.0",
    "@styled-icons/remix-fill": "^10.18.0",
    "dotenv": "^10.0.0",
    "graphql": "^15.5.1",
    "joi": "^17.4.0",
    "lodash": "^4.17.21",
    "next": "11.0.1",
    "next-auth": "^3.27.2",
    "next-pwa": "^5.2.23",
    "next-seo": "^4.26.0",
    "nextjs-progressbar": "^0.0.11",
    "polished": "^4.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-slick": "^0.28.1",
    "styled-components": "^5.3.0",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@jackfranklin/test-data-bot": "^1.3.0",
    "@next/eslint-plugin-next": "^11.0.1",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/react": "^6.3.2",
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/user-event": "^13.1.9",
    "apollo": "^2.33.4",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.1",
    "cypress": "7.6.0",
    "cypress-plugin-stripe-elements": "^1.0.1",
    "eslint": "^7.30.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-styled-components": "^7.0.4",
    "lint-staged": "^11.0.0",
    "msw": "^0.30.1",
    "node-fetch": "^2.6.1",
    "plop": "^2.7.4",
    "prettier": "^2.3.2",
    "react-is": "^17.0.2",
    "setimmediate": "^1.0.5",
    "start-server-and-test": "^1.12.6",
    "storybook-addon-next-router": "^2.0.4"
  }
}
